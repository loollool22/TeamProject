	<insert id="insertMember">

		insert into member(id,pass,name,date) values(#{id},#{pass},#{name},#{date})
<!-- 		memberDTO안에 있는 id, pass, name, date 값을 자동으로 가져온다. -->
	</insert>
<!-- 	우리는 우선 insert 구문 만들어 줄 거기 때문에 아래는 주석 -->
<!-- 	<update id=""></update> -->
<!-- 	<delete id=""></delete> -->
<!-- 	<select id=""></select> -->

<!-- 		insert into member(id,pass,name,date) value(?,?,?,?) -->
<!-- 		물을표 부분에 멤버변수 이름을 넣어준다. -->
</mapper>